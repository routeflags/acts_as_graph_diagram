<p id="notice"><%= notice %></p>

<h1>Gods</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gods.each do |god| %>
      <tr>
        <td><%= link_to 'Show', god %></td>
        <td><%= link_to 'Edit', edit_god_path(god) %></td>
        <td><%= link_to 'Destroy', god, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New God', new_god_path %>
